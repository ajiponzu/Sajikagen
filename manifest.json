{
    "name": "Sajikagen",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "e-kagen",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "recognize.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ]
}