{
    "name": "Sajikagen",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "e-kagen",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "recognize.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "popup.js"
        ]
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}